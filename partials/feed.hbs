<script>if(window.feedConfig){const e={twitter:"twitter",instagram:"instagram",github:"github",telegram:"telegram",ncm:"ncm"};fetch(window.feedConfig.server+"?"+new URLSearchParams(window.feedConfig)).then(e=>e.json()).then(t=>{let r=document.getElementById("user-feeds"),n=document.getElementsByClassName("feed")[0],i=window._sys.img_url;i=i.substring(0,i.indexOf("?"));for(let o of t){let t=n.cloneNode(!0);t.querySelector(".feed-icon").setAttribute("src",i+e[o.source]+".svg"),t.querySelector(".feed-icon").setAttribute("alt",o.source),t.querySelector(".feed-title").textContent=o.title,t.querySelector(".feed-date").setAttribute("datetime",o.date),t.querySelector(".feed-date").textContent=new Date(o.date).toLocaleDateString("en"),t.querySelectorAll(".feed-link").forEach(e=>e.setAttribute("href",o.link)),t.style.display="grid",r.append(t)}})}</script>